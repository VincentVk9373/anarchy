<div class="define-item-type item anarchy-quality {{#unless data.positive}}negative{{/unless}} {{#if data.inactive}}item-inactive{{/if}}" data-item-id="{{_id}}" data-item-type="quality" draggable="true">
  <img class="anarchy-img item-img" src="{{img}}" alt="{{name}}" title="{{name}}"/>
  <div class="flexcol">
    <label>{{name}}</label>
    <label class="item-description">{{{data.description}}}</label>
  </div>
  {{> 'systems/anarchy/templates/common/item-controls.hbs'}}
</div>
