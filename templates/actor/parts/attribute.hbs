<div class="flexcol item style-attribute" data-attribute="{{key}}">
  <a class="attribute-label">
    <label for="data.attributes.{{key}}.value">{{localize (concat 'SRA.attributes.' key)}}</label>
  </a>
  <input class="attribute-value" type="text" data-dtype="Number"
    name="data.attributes.{{key}}.value" value="{{attribute.value}}" 
  />
</div>
