{{!
  data is:
  {
    // the entries to propose as options
    entries: [
      { "value": "k1", "label": "ANARCHY.(...).label1"},
      { "value": "k2", "label": "ANARCHY.(...).label2"},
    ]
  }
}}
{{#unless noEmpty}}
<option value=""></option>
{{/unless}}
{{#each entries as |entry|}}
<option value="{{entry.value}}">{{#if entry.labelkey}}{{localize entry.labelkey}}{{else}}{{entry.label}}{{/if}}</option>
{{/each}}
